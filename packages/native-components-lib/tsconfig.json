{
    "compilerOptions": {
        "outDir": "dist",
        "rootDir": "src",
        "module": "Node16",
        "target": "ESNext",
        "lib": ["DOM", "ESNext"],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "declaration": true,
        "declarationDir": "dist",
        "declarationMap": true,
        "emitDeclarationOnly": false,
        "jsx": "react-jsx",
        "noEmit": false
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
}
